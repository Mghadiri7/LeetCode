   "source": [
    "def repeatedStringMatch(a, b):\n",
    "    repeat = (len(b) // len(a))\n",
    "    for i in range(3):\n",
    "        if b in a * (repeat + i):\n",
    "            return repeat + i\n",
    "    return -1"
   ]
  },
