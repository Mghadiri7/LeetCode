   "source": [
    "import heapq\n",
    "\n",
    "def lastStoneWeight(stones):\n",
    "    h = [-x for x in stones]\n",
    "    heapq.heapify(h)\n",
    "    while len(h)>1:\n",
    "        heapq.heappush(h, heapq.heappop(h) - heapq.heappop(h))\n",
    "    return -h[0]"
   ]
  },
