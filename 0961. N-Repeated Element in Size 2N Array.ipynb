   "source": [
    "def repeatedNTimes(nums):\n",
    "    n = len(nums)\n",
    "    for i in range(n-2):\n",
    "        if nums[i]==nums[i+1] or nums[i]==nums[i+2]:\n",
    "            return nums[i]\n",
    "    else:\n",
    "        return nums[-1]"
   ]
