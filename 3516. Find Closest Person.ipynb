   "source": [
    "def findClosest(x, y, z):\n",
    "    first, second = abs(z-x), abs(z-y)\n",
    "    if first<second:\n",
    "        return 1\n",
    "    elif second<first:\n",
    "        return 2\n",
    "    else:\n",
    "        return 0"
   ]
