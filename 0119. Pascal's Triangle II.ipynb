   "source": [
    "def getRow(rowIndex):\n",
    "    ans = [1]\n",
    "    pre = ans[0]\n",
    "    for i in range(rowIndex):\n",
    "        pre = pre * (rowIndex - i) // (i + 1)\n",
    "        ans.append(pre)\n",
    "    return ans"
   ]
