   "source": [
    "def singleNumber(nums):\n",
    "    ones = 0\n",
    "    twos = 0\n",
    "    for num in nums:\n",
    "        ones ^= (num & ~twos)\n",
    "        twos ^= (num & ~ones)\n",
    "    return ones"
   ]
  },
