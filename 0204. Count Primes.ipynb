   "source": [
    "def countPrimes(n):\n",
    "    seen = [0] * n\n",
    "    ans = 0\n",
    "    for i in range(2, n):\n",
    "        if seen[i]==0:\n",
    "            ans+=1\n",
    "            for j in range(i*2, n, i):\n",
    "                seen[j] = 1\n",
    "    return ans"
   ]
