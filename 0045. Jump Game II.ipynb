   "source": [
    "def jump(nums):\n",
    "    n = len(nums)-1\n",
    "    end, farthest, jump = 0, 0, 0\n",
    "    for i in range(n):\n",
    "        farthest = max(farthest, nums[i]+i)\n",
    "        if end==i:\n",
    "            jump+=1\n",
    "            end = farthest\n",
    "    return jump"
   ]
