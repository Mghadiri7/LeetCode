   "source": [
    "def grayCode(n):\n",
    "    total_number = 1<<n\n",
    "    result = []\n",
    "    for i in range(total_number):\n",
    "        shifted_i = i>>1\n",
    "        gray_val = i ^ shifted_i\n",
    "        result.append(gray_val)\n",
    "    return result"
   ]
  },
