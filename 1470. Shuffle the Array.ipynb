   "source": [
    "def shuffle(nums, n):\n",
    "    ans = [0] * n * 2\n",
    "    for i in range(n):\n",
    "        ans[2*i], ans[2*i+1] = nums[i], nums[i+n]\n",
    "    return ans"
   ]
  },
