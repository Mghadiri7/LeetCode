   "source": [
    "def canMakeArithmeticProgression(arr):\n",
    "    arr.sort()\n",
    "    n = len(arr) - 1\n",
    "    diff = arr[0] - arr[1]\n",
    "    for i in range(1, n):\n",
    "        if arr[i]-arr[i+1] != diff:\n",
    "            return False\n",
    "    return True"
   ]
