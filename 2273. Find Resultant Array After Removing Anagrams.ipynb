   "source": [
    "def removeAnagrams(words):\n",
    "    n = len(words)\n",
    "    res = [words[0]]\n",
    "    for i in range(1, n):\n",
    "        if sorted(words[i]) != sorted(words[i-1]):\n",
    "            res.append(words[i])\n",
    "    return res"
   ]
  },
