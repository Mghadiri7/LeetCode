   "source": [
    "def countTriples(n):\n",
    "    sq = {i*i for i in range(1, n+1)}\n",
    "    ans = 0\n",
    "    for i in range(1, n-1):\n",
    "        a = i*i\n",
    "        for j in range(i+1, n):\n",
    "            c = a + j*j\n",
    "            if c in sq:\n",
    "                ans+=2\n",
    "    return ans"
   ]
