    "def finalValueAfterOperations(operations):\n",
    "    x = 0\n",
    "    for item in operations:\n",
    "        if item==\"++X\" or item==\"X++\":\n",
    "            x+=1\n",
    "        else:\n",
    "            x-=1\n",
    "    return x"
   ]
  },
