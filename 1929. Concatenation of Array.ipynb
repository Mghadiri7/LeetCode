   "source": [
    "def getConcatenation(nums):\n",
    "    n = len(nums)\n",
    "    ans = [0] * 2 * n\n",
    "    for i in range(n):\n",
    "        ans[i] = nums[i]\n",
    "        ans[i+n] = nums[i]\n",
    "    return ans"
   ]
  },
