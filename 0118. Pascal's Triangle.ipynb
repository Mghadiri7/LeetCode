   "source": [
    "def generate(numRows):\n",
    "    ans = [[1]]\n",
    "    for i in range(1, numRows):\n",
    "        lst = [1]\n",
    "        for j in range(i-1):\n",
    "            lst.append(ans[i-1][j] + ans[i-1][j+1])\n",
    "        lst.append(1)\n",
    "        ans.append(lst)\n",
    "    return ans"
   ]
